import random
n=random.randint(1,99)
guess=int(input("Enter number from 1 to 99"))
while n!='guess':
    print()
    if guess<n:
        print("Guess is low")
        guess=int(input("Try again"))
    elif guess>n:
        print("Guess is high")
        guess=int(input("Try again"))
    else:
        print("You guessed it")
        break
    print()
